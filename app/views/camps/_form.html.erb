<%= simple_form_for(@camp) do |f| %>
<%= f.error_notification %>

  <div class="row"> 
    <div class="form-inputs">

      <%= f.input :location_id, collection: Location.alphabetical.active.to_a, include_blank: true%>
      <%= f.input :curriculum_id, collection: Curriculum.alphabetical.active.to_a, include_blank: true%>
      <%= f.input :cost, label_html: { class: 'active' } %>
      <%= f.input :start_date, label_html: { class: 'active' } %> 
      <%= f.input :end_date, label_html: { class: 'active' } %> 
      <%= f.input :time_slot, collection: [['Morning', 'am'], ['Afternoon', 'pm']] %> 
      <%= f.input :max_students, label_html: { class: 'active' } %> 
      <%= f.input :active%>

    </div>
  </div>

  <div class="row">
    <div class="form-actions col s8">
      <%= f.button :submit, class: 'waves-effect waves-light btn cyan' %>
    </div>
  </div>
   
<% end %>

